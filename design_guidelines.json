{
  "identity": {
    "name": "E1",
    "role": "The Anti-AI Designer",
    "principles": [
      "Fusion over Imitation",
      "Emotion First",
      "Content is King",
      "Zoom In",
      "Negative Space",
      "Contrast"
    ]
  },
  "typography": {
    "headings": {
      "font_family": "Playfair Display, serif",
      "weights": ["400", "600", "700"],
      "usage": "Main headings (H1-H3), Hero sections",
      "letter_spacing": "tracking-tight"
    },
    "body": {
      "font_family": "Manrope, sans-serif",
      "weights": ["400", "500", "600"],
      "usage": "Paragraphs, UI text, Labels",
      "letter_spacing": "tracking-normal"
    },
    "monospace": {
      "font_family": "JetBrains Mono, monospace",
      "usage": "Code snippets, Order IDs, Technical data"
    },
    "scale": {
      "h1": "text-5xl md:text-7xl font-bold leading-tight",
      "h2": "text-4xl md:text-5xl font-semibold leading-tight",
      "h3": "text-2xl md:text-3xl font-medium",
      "body_lg": "text-lg md:text-xl leading-relaxed",
      "body_base": "text-base leading-relaxed",
      "caption": "text-sm text-muted-foreground uppercase tracking-widest"
    }
  },
  "colors": {
    "theme": "light",
    "palette": {
      "primary": {
        "DEFAULT": "#2D4A3E",
        "foreground": "#FFFFFF",
        "description": "Deep Forest Green - Trust, Growth, Stability"
      },
      "secondary": {
        "DEFAULT": "#D4A373",
        "foreground": "#000000",
        "description": "Earthy Terracotta/Sand - Organic, Grounded"
      },
      "accent": {
        "DEFAULT": "#E9C46A",
        "foreground": "#000000",
        "description": "Harvest Gold - Optimism, Freshness"
      },
      "background": {
        "DEFAULT": "#FAF9F6",
        "paper": "#FFFFFF",
        "subtle": "#F0F1EE"
      },
      "text": {
        "primary": "#1A1C1A",
        "secondary": "#4A4F4A",
        "muted": "#8C928C"
      },
      "status": {
        "success": "#40916C",
        "warning": "#E9C46A",
        "error": "#D9534F",
        "info": "#52B788"
      }
    },
    "gradients": {
      "hero": "bg-gradient-to-b from-[#FAF9F6] to-[#E8EBE8]",
      "card_hover": "bg-gradient-to-br from-white to-[#F0F1EE]"
    }
  },
  "spacing": {
    "scale": ["p-1", "p-2", "p-4", "p-6", "p-8", "p-12", "p-16", "p-20", "p-24", "p-32"],
    "container_padding": "px-6 md:px-12 lg:px-24",
    "section_spacing": "py-20 md:py-32",
    "element_spacing": "gap-6 md:gap-8"
  },
  "layout": {
    "marketing": {
      "grid": "grid-cols-1 md:grid-cols-12 gap-8",
      "modes": {
        "tetris": "Asymmetrical grid for visual interest. Mix col-span-4, col-span-8, col-span-6."
      }
    },
    "dashboard": {
      "grid": "grid-cols-1 md:grid-cols-3 lg:grid-cols-4 gap-6",
      "modes": {
        "bento": "Dense grid. Summary cards (span-1), Main charts (span-2 or 3), Feeds (row-span-2)."
      }
    },
    "container": {
      "max_width": "max-w-7xl mx-auto"
    }
  },
  "components": {
    "buttons": {
      "primary": "bg-primary text-primary-foreground rounded-full px-8 py-3 hover:bg-primary/90 transition-all shadow-lg hover:shadow-xl hover:-translate-y-0.5",
      "secondary": "bg-white border border-input text-primary hover:bg-accent/10 rounded-full px-8 py-3 transition-all",
      "ghost": "hover:bg-accent/10 text-primary rounded-full px-6 py-2"
    },
    "cards": {
      "base": "bg-white rounded-2xl border border-border/40 shadow-sm hover:shadow-md transition-all duration-300",
      "marketing": "bg-[#F0F1EE] rounded-3xl border-none overflow-hidden",
      "dashboard": "bg-white rounded-xl border border-border/60 shadow-sm"
    },
    "inputs": {
      "base": "rounded-lg border-border/60 bg-white/50 focus:ring-2 focus:ring-primary/20 focus:border-primary transition-all"
    }
  },
  "visual_enhancers": {
    "radius": {
      "default": "rounded-xl",
      "large": "rounded-3xl",
      "full": "rounded-full"
    },
    "shadows": {
      "soft": "shadow-[0_8px_30px_rgb(0,0,0,0.04)]",
      "hover": "shadow-[0_8px_30px_rgb(0,0,0,0.12)]"
    },
    "textures": {
      "grain": "bg-[url('https://grainy-gradients.vercel.app/noise.svg')] opacity-20 mix-blend-soft-light pointer-events-none fixed inset-0 z-50"
    }
  },
  "motion": {
    "transitions": "transition-all duration-300 ease-out",
    "hover_lift": "hover:-translate-y-1",
    "entrance": {
      "fade_in_up": "animate-in fade-in slide-in-from-bottom-4 duration-700"
    }
  },
