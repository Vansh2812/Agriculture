  "icons": {
    "primary_library": "lucide-react",
    "marketing_library": "@phosphor-icons/react",
    "usage": "Use thin strokes (1.5px) for elegance. Use filled icons for active states."
  },
  "universal_guidelines": [
    "Avoid 'AI Slop': No generic centered layouts, no default blue/purple gradients.",
    "Dark colors must be solid, not gradients.",
    "Use glassmorphism with purpose (backdrop-blur-xl).",
    "No 'transition: all'. Animate specific properties.",
    "No text-align: center on the main App container.",
    "Use 'sonner' for toasts.",
    "Interactive elements must have data-testid.",
    "Use 'Playfair Display' for headings to give a premium, organic feel.",
    "Use 'Manrope' for body text for modern readability."
  ],
  "instructions_to_main_agent": [
    "1. Install dependencies: yarn add @phosphor-icons/react lucide-react sonner clsx tailwind-merge",
    "2. Configure Tailwind: Update tailwind.config.js with the new color palette and font families.",
    "3. Set up Fonts: Import Playfair Display and Manrope in index.css or via Google Fonts.",
    "4. Create Layouts: Build a 'MarketingLayout' (Tetris grid) and 'DashboardLayout' (Sidebar + Bento grid).",
    "5. Build Components: Customize Shadcn components to match the 'Organic & Earthy' theme (rounded-xl, soft shadows).",
    "6. Implement Pages: Start with the Landing Page using the Hero images and Parallax effects.",
    "7. Dashboard: Implement the Farmer and Buyer dashboards using the Bento grid system.",
    "8. Ensure Accessibility: Check contrast ratios, especially with the green primary color."
  ]
}